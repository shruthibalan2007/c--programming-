#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>
int main() 
{
int a,i,age;
int young=100;
int old=-1;
int invalid=0;
scanf("%d",&a);
if(a<=0)
{
printf("Invalid Input\n");
return 0;
}
for(i=0;i<a;i++)
{
scanf("%d",&age);
if(age<0)
{
invalid=1;
break;
}
if(age<young)
young=age;
if(age>old)
old=age;
}
if(invalid)    
{
printf("Invalid Input\n");
}
else
{
printf("Youngest=%d\n",young);
printf("Oldest=%d\n",old);
}
return 0;
}
