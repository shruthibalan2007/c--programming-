#include <stdio.h>
int main()
{
int n,address,found = 0;
scanf("%d",&n);
int doors[n];    
for (int i=0;i<n;i++)
{
scanf("%d",&doors[i]);
}  
scanf("%d",&address);
for (int i=0;i<n;i++)
{
if (doors[i]==address)
{
printf("Door Number is %03d-DN\n",i);
found = 1;
break;
}
}    
if (!found)
{
printf("-1\n");
}
return 0;
}
