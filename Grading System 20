#include <stdio.h>

int main() {
    char name[50];
    int marks[5];
    int total = 0;
    float average;
    char grade[10];
    scanf("%s", name);
    for (int i = 0; i < 5; i++) {
        scanf("%d", &marks[i]);
        total += marks[i];
    }
    average = total /5.0;
    if (average >= 90)
        sprintf(grade, "A");
    else if (average >= 80)
        sprintf(grade, "B");
    else if (average >= 70)
        sprintf(grade, "C");
    else if (average >= 60)
        sprintf(grade, "D");
    else if (average >= 50)
        sprintf(grade, "E");
    else
        sprintf(grade, "Fail");
    printf("Name of the Student:%s\n", name);
    printf("Total Mark:%d\n", total);
    printf("Average Mark:%.1f\n", average);
    printf("Grade Mark:%s\n", grade);

    return 0;
}
