#include <stdio.h>
int main() 
{
int n, i;
long long int sum_of_squares = 0; 
scanf("%d", &n);
int arr[n]; 
for (i = 0; i < n; i++) 
{
scanf("%d", &arr[i]);
}
for (i = 0; i < n; i++)
{
if (arr[i] > 0) 
{ 
sum_of_squares += (long long int)arr[i] * arr[i];
}
}
printf("%lld\n", sum_of_squares);
return 0;
}
